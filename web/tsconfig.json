{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "$/*": ["src/*"],
      "$types": ["src/types.ts"],
      "$components/*": ["src/components/*"],
      "$stores/*": ["src/stores/*"],
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"],
      "$apis/*": ["src/apis/*"]
    }
  }
}
